package unit;
import "generic/vector3.schema";


component AiUnit {
    id = 128;
    AiUnitStateEnum current_state = 1;
    int64 aggroed_unit_id = 2;
    generic.Vector3f aggroed_unit_coordinate = 3;
}

enum AiUnitStateEnum {

	idle = 0;
    aggroed = 1;
    chasing = 2;
    attacking = 3;
    returning = 4;

}

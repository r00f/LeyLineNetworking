{
  "build": {
    "tasks": [
      {
        "name": "Codegen",
        "steps": [{"name": "** Load Unity to generate code **", "command": "echo", "arguments": ["No-op."]}]
      },
      {
        "name": "build",
        "steps": [
          {
            "name": "** Run ci/build-gamelogic.sh instead **",
            "command": "echo",
            "arguments": [ "No-op." ]
          }
        ]
      },
      {
        "name": "clean",
        "steps": [{"name": "No-op", "command": "echo", "arguments": ["No-op."]}]
      }
    ]
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "MapSpawn"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 500
      }
    },
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkoutAllInitially": false
    }
  },
  "external": {
    "default": {
      "run_type": "EXECUTABLE",
      "windows": {
        "command": "build/worker/MapSpawn@Windows/MapSpawn@Windows.exe",
        "arguments": [
          "+workerType",
          "MapSpawn",
          "-batchmode",
          "-nographics",
          "-logfile",
          "../../logs/external-default-MapSpawn.log"
        ]
      },
      "macos": {
        "command": "open",
        "arguments": [
          "-n",
          "./build/worker/MapSpawn@Mac/MapSpawn@Mac.app",
          "--args",
          "+workerType",
          "MapSpawn",
          "-batchmode",
          "-nographics",
          "-logfile",
          "../../logs/external-default-MapSpawn.log"
        ]
      }
    }
  },
  "managed": {
    "windows": {
      "artifact_name": "MapSpawn@Windows.zip",
      "command": "MapSpawn@Windows.exe",
      "arguments": [
        "+workerType",
        "MapSpawn",
        "+workerId",
        "${IMPROBABLE_WORKER_ID}",
        "+receptionistHost",
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "+receptionistPort",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "+linkProtocol",
        "Tcp",
        "-batchmode",
        "-nographics",
        "-logfile",
        "${IMPROBABLE_LOG_FILE}"
      ]
    },
    "macos": {
      "artifact_name": "MapSpawn@Mac.zip",
      "command": "MapSpawn@Mac.app/Contents/MacOS/MapSpawn@Mac",
      "arguments": [
        "+workerType",
        "MapSpawn",
        "+workerId",
        "${IMPROBABLE_WORKER_ID}",
        "+receptionistHost",
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "+receptionistPort",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "+linkProtocol",
        "Tcp",
        "-batchmode",
        "-nographics",
        "-logfile",
        "${IMPROBABLE_LOG_FILE}"
      ]
    },
    "linux": {
      "artifact_name": "MapSpawn@Linux.zip",
      "command": "MapSpawn@Linux",
      "arguments": [
        "+workerType",
        "MapSpawn",
        "+workerId",
        "${IMPROBABLE_WORKER_ID}",
        "+receptionistHost",
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "+receptionistPort",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "+linkProtocol",
        "Tcp",
        "-batchmode",
        "-nographics",
        "-logfile",
        "${IMPROBABLE_LOG_FILE}"
      ]
    }
  }
}
